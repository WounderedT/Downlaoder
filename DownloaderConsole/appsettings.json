{
  "DomainProcessors": {
    "joyreactor.cc": {
      "TagPageProcessingDelayMinSec": 1,
      "TagPageProcessingDelayMaxSec": 10
    }
  },

  "ProgressHandler": {
    "ShowPopupNotifications": false
  },

  "DownloadHandler": {
    "Enabled": true,
    "RetryOnFailure": true,
    "SkipMissing": true,
    "StopOnError": false,
    "DownloadExtension": ".download",
    "RetryPolicyByDomain": {
      "joyreactor.cc": {
        "Retries": 3,
        "ConcurrentProcessingScale": "None"
      },
      "cyberdrop.me": {
        "MaxParallelProcessingCount": 2,
        "Retries": 15,
        "ConcurrentProcessingScale": "Half"
      },
      "fastpic.org": {
        "Retries": 1,
        "ConcurrentProcessingScale": "None"
      },
      "*": {
        "Retries": 5,
        "ConcurrentProcessingScale": "None"
      }
    },
    "Strategies": {
      "Default": {

      },
      "Fragmented": {
        "Domains": ["cyberdrop.me"]
      }
    }
  },

  "DownloaderConsole": {
    "Resume": true
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "Debug",
          "path": "Logs\\log-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}|{Level:u4}|{ThreadId}|{SourceContext}] --- {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log-.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  }
}